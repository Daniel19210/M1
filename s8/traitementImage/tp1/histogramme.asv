function histoImg = histogramme(image)
%HISTOGRAMME Summary of this function goes here
%   Detailed explanation goes here
[m, n, can] = size(image);
hist = zeros(m*n);

if(can > 1)
    imageNG = rgb2gray(image);
else
    imageNG = image;
end

for i = 1:m
    for j = 1:n
        hist(imageNG(i, j)) = hist(imageNG(i, j)) + 1;
    end
end

histoImg = hist;
imagesc()
end

